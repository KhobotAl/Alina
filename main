#include <iostream> //підключення стандартної бібліотеки
using namespace std; //використання пространства імен

int mail() // заголовок основної функції
{  //початок виконуваної частини
  int a; //оголошення змінної
  cout << "Enter valua a : "; //виведення даних на консоль
  cin >> a; //введення змінної з консолі
  int res = a - 5; //розрахунок результату
  cout << "a-5: "<<res; //виведення результату на консоль
  return 0; //повернення
//кінець
